{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": ["eslint:recommended","prettier","plugin:prettier/recommended"],
    "parser": "babel-eslint",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "rules": {
    "prettier/prettier": "error",
    "no-console": "off"
    }
}
